BLOCKING
10% skimmed milk proteins in PBS (pH 7.4) },
	Author = {Jacob, Asha and van IJzendoorn, Leo J. and de Jong, Arthur M. and Prins, Menno W.J.},
	Date-Added = {2017-08-23 18:44:50 +0000},
