	Keywords = {aggregation; fusion protein; glutathione S-transferase; inclusion bodies; maltose-binding protein; protein folding; solubility; thioredoxin},
	Number = {8},
	Pages = {1668--1674},
	Publisher = {Cold Spring Harbor Laboratory Press},
	Title = {Escherichia coli maltose-binding protein is uncommonly effective at promoting the solubility of polypeptides to which it is fused},
