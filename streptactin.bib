	Journal = {Protein Expression and Purification},
	Month = {Nov},
	Number = {2},
	Pages = {192--196},
	Publisher = {Elsevier BV},
	Title = {Production of a Soluble and Functional Recombinant Streptavidin in Escherichia coli},
	Url = {http://dx.doi.org/10.1006/prep.1998.0930},
	Volume = {14},
	Year = {1998},
